#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int num,half;
    scanf("%d",&num);
    scanf("%d",&half);
    while(half>1)
    {
        num=num/2;
        half--;
    }
    printf("%d",num);
    return 0;
}
